{
  "name": "mabasic/mariobasic-n7",
  "description": "My personal website built with Phel.",
  "authors": [
    {
      "name": "Mario Bašić",
      "email": "mario@laravelista.hr"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^8.0",
    "aws/aws-sdk-php": "^3.198",
    "erusev/parsedown-extra": "^0.8.1",
    "mabasic/phel-json": "^0.4",
    "phel-lang/phel-lang": "^0.5",
    "rakibtg/sleekdb": "^2.13",
    "spatie/yaml-front-matter": "^2.0",
    "vlucas/phpdotenv": "^5.3"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "prefer-stable": true,
  "scripts": {
    "dev": "php -S localhost:8000 -t public/",
    "pre-install-cmd": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "test": "vendor/bin/phel test"
  }
}
